{
  "python.pythonPath": "python3",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.linting.mypyEnabled": true,
  "python.linting.pytypeEnabled": true,
  "python.linting.pylintEnabled": false,
  "pyright.inlayHints.variableTypes": true,
  "python.linting.mypyArgs": [
    "--ignore-missing-imports",
    "--follow-imports=silent",
    "--show-column-numbers"
  ],
  "codeLens.enable": true,
  "python.formatting.blackArgs": [],
  "pyright.inlayHints.functionReturnTypes": true,
  "python.analysis.diagnosticSeverityOverrides": {
  },
  "pyright.enable": true,
  "suggest.autoTrigger": "none",
  "suggest.insertMode": "insert",
  "suggest.noselect": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/ansible-community/schemas/main/f/ansible-playbook.json": "khp-ansible/*.yml",
    "https://json.schemastore.org/kustomization.json": "/kustomization.yaml",
    "kubernetes": ["/*.yaml", "!/kustomization.yaml"]
  },
  "yaml.customTags": ["!vault scalar"],
  "yaml.trace.server": "verbose",
  "java.enabled": false,
  "java.compile.nullAnalysis.mode": "automatic",
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
